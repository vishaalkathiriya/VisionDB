  <DataSets>
    <DataSet Name="Expenses">
      <Query>
        <DataSourceName>VisionDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@practiceId">
            <Value>=Parameters!practiceId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start">
            <Value>=Parameters!start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select isnull(e.Category, '(No Category)') Category, sum(e.Cost) Cost, count(*) CategoryCount from Expenses e 
where e.practice_Id = @practiceId and e.Deleted is null 
and e.ExpenseDate &gt;= @start and e.ExpenseDate &lt;= @end 
group by e.Category
order by e.Category</CommandText>
      </Query>
      <Fields>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Cost">
          <DataField>Cost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CategoryCount">
          <DataField>CategoryCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
