  <DataSets>
    <DataSet Name="AuditLog">
      <Query>
        <DataSourceName>VisionDB</DataSourceName>
        <CommandText>select top 2000 al.Added, al.UserName, al.EntryType, al.EntryText, p.Name from AuditLog al
left join Customers c on c.Id = al.EntryId
left join Practices p on p.Id = c.practice_Id
where al.EntryType = 'Patient_Record_Viewed' 
union
select al.Added, al.UserName, al.EntryType, al.EntryText, p.Name from AuditLog al
left join AspNetUsers u on u.Id = al.EntryId
left join Practices p on p.Id = u.practiceId
where al.EntryType != 'Patient_Record_Viewed' 
order by al.Added desc</CommandText>
      </Query>
      <Fields>
        <Field Name="Added">
          <DataField>Added</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EntryType">
          <DataField>EntryType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EntryText">
          <DataField>EntryText</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
