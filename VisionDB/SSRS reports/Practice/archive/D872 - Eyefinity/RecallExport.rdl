  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>VisionDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@practiceId">
            <Value>=Parameters!practiceId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start">
            <Value>=Parameters!start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
c.Title + ' ' + c.Firstnames + ' ' + c.Surname PatientName,
c.Address,
c.Postcode,
c.Telephone,
c.SMSNumber,
c.Email,
c.PreviousEyeExamDate,
c.NextDueDateEyeExam
from Customers c
where c.practice_Id = @practiceId 
and c.NextDueDateEyeExam &gt; @start
and c.NextDueDateEyeExam &lt; DATEADD(day, 1, @end)
order by NextDueDateEyeExam</CommandText>
      </Query>
