  <DataSources>
    <DataSource Name="VisionDB">
      <DataSourceReference>/Datasources/VisionDB</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>e85656d7-8f6b-4e55-ab8e-68667ec0e421</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="VisionDB">
      <Query>
        <DataSourceName>VisionDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start">
            <Value>=Parameters!start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@practiceId">
            <Value>=Parameters!practiceId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
isnull(c.Title + ' ', '') + c.Firstnames + ' ' + c.Surname + isnull(' - ' + c.Number, '') PatientNameAndNo,
c.DOB,
replace(c.Address, char(13) + char(10), ', ') + ' ' + c.Postcode Address,
isnull(c.CareHome, c.ShelteredAccommodation) CareHomeOrShelteredAccommodation,
ee.TestDateAndTime
from EyeExams ee
inner join Customers c on c.Id = ee.customer_Id
where ee.TestDateAndTime &gt; @start and ee.TestDateAndTime &lt; @end
and ee.Deleted is null
and c.practice_Id = @practiceId
order by ee.TestDateAndTime</CommandText>
      </Query>
      <Fields>
        <Field Name="PatientNameAndNo">
          <DataField>PatientNameAndNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOB">
          <DataField>DOB</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Address">
