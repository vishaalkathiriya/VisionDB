  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>VisionDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@practiceId">
            <Value>=Parameters!practiceId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start">
            <Value>=Parameters!start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
a.Title, a.Notes, a.Start, a.[End], 
REPLACE(REPLACE(c.Address, CHAR(13), ', '), CHAR(10), '') + ', ' + c.Postcode PatientAddress,
c.Telephone,
c.SMSNumber,
c.DOB,
c.Comments
from Appointments a 
left join Customers c on c.Id = a.customer_Id
where a.practice_Id = @practiceId and a.Start &gt; @start and a.Start &lt; @end 
and a.Deleted is null
order by a.Start</CommandText>
      </Query>
      <Fields>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
