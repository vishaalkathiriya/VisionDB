          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--SALES

declare @InvoiceId uniqueidentifier

DECLARE @sales_raw TABLE(
Id uniqueidentifier,
invoice_date datetime,
invoice_number NVARCHAR(50),
patient_name NVARCHAR(200),
patient_number NVARCHAR(20),
dispensed_sales float,
adhoc_sales float,
[services] float,
professional_fees float,
card_payment float,
cash_payment float,
cheque float
)

DECLARE @ReportData CURSOR
SET @ReportData = CURSOR FOR
select i.Id from Invoices i
inner join InvoiceDetails id on id.invoice_Id = i.Id
where i.practice_Id = @practiceId and i.Deleted is null and i.InvoiceDate &gt; @start and i.InvoiceDate &lt; @end
group by i.Id
OPEN @ReportData
FETCH NEXT
FROM @ReportData INTO @InvoiceId
WHILE @@FETCH_STATUS = 0
BEGIN

declare @customerId uniqueidentifier = null
declare @invoice_date datetime = null
declare @invoice_number nvarchar(50) = null
select @customerId = i.customer_Id, @invoice_date = i.InvoiceDate, @invoice_number = i.InvoiceNumber from Invoices i where i.Id = @InvoiceId 

declare @patient_name nvarchar(200) = null
declare @patient_number nvarchar(20) = null
select @patient_name = isnull(c.Title, '') + ' ' + c.Firstnames + ' ' + c.Surname, @patient_number = c.Number from Customers c where c.Id = @customerId
IF (@patient_name is null)
BEGIN
	set @patient_name = 'Ad hoc'
END

declare @dispensed_sales float = 0
declare @adhoc_sales float = 0
declare @services float = 0
declare @professional_fees float = 0
declare @card_payment float = 0
declare @cash_payment float = 0
declare @cheque float = 0

--frames and lenses are dispensed
select @dispensed_sales = isnull(sum(id.UnitPrice * id.Quantity * (1 - (DiscountPercentage / 100))), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (1, 2)
--contact lenses and tints can be dispensed
select @dispensed_sales = @dispensed_sales + isnull(sum(id.UnitPrice * id.Quantity * (1 - (DiscountPercentage / 100))), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (8, 9) and id.Dispensed = 1

--accessories and sunglasses
select @adhoc_sales = isnull(sum(id.UnitPrice * id.Quantity * (1 - (DiscountPercentage / 100))), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (0, 7)
--contact lenses and tints may not be dispensed in some cases
select @adhoc_sales = @adhoc_sales + isnull(sum(id.UnitPrice * id.Quantity * (1 - (DiscountPercentage / 100))), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (8, 9) and id.Dispensed = 0

--services
select @services = isnull(sum(id.UnitPrice * id.Quantity * (1 - (DiscountPercentage / 100))), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (3)

--professional fees
select @professional_fees = isnull(sum(id.UnitPrice * id.Quantity * (1 - (DiscountPercentage / 100))), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (10)

--card payment
select @card_payment = abs(isnull(sum(id.UnitPrice * id.Quantity), 0)) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (5) and (p.Name like '%card%' or p.Name like '%pdq%')

--cash payment
select @cash_payment = abs(isnull(sum(id.UnitPrice * id.Quantity), 0)) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (5) and p.Name like '%cash%'

--cheque payment
select @cheque = abs(isnull(sum(id.UnitPrice * id.Quantity), 0)) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (5) and (p.Name like '%cheque%' or p.Name like '%chq%')

insert into @sales_raw (Id, invoice_date, invoice_number, patient_name, patient_number, dispensed_sales, adhoc_sales, [services], professional_fees, card_payment, cash_payment, cheque) 
values (@InvoiceId, @invoice_date, @invoice_number, @patient_name, @patient_number, @dispensed_sales, @adhoc_sales, @services, @professional_fees, @card_payment, @cash_payment, @cheque)

FETCH NEXT
FROM @ReportData INTO @InvoiceId
END
CLOSE @ReportData
DEALLOCATE @ReportData

DECLARE @sales TABLE(
invoice_date datetime,
invoice_count int,
dispensed_sales float,
adhoc_sales float,
[services] float,
professional_fees float,
card_payment float,
cash_payment float,
cheque float
)

insert into @sales
select cast(invoice_date as date) invoice_date_grouped, count(*) invoice_count, 
sum(dispensed_sales) dispensed_sales, sum(adhoc_sales) adhoc_sales, sum([services]) [services], 
sum(professional_fees) professional_fees, sum(card_payment) card_payment, sum(cash_payment) cash_payment, sum(cheque) cheque
from @sales_raw  
group by cast(invoice_date as date) 

--EXPENSES
DECLARE @non_vat_expenses TABLE(
expense_date datetime,
expense_count int,
expense float
)

insert into @non_vat_expenses
select cast(e.ExpenseDate as date), count(*) expense_count, SUM(e.Cost) from Expenses e 
where e.practice_Id = @practiceId and Deleted is null and e.ExpenseDate &gt; @start and e.ExpenseDate &lt; DATEADD(day, 1, @end) and e.VATRate = 0
group by cast(e.ExpenseDate as date)


DECLARE @vat_expenses TABLE(
expense_date datetime,
expense_count int,
expense float
)

insert into @vat_expenses
select cast(e.ExpenseDate as date), count(*) expense_count, SUM(e.Cost) from Expenses e 
where e.practice_Id = @practiceId and Deleted is null and e.ExpenseDate &gt; @start and e.ExpenseDate &lt; DATEADD(day, 1, @end) and e.VATRate &gt; 0
group by cast(e.ExpenseDate as date)


--COMBINE SALES AND EXPENSES DATA
DECLARE @salesandexpenses TABLE(
[date] datetime,
invoice_count int,
dispensed_sales float,
adhoc_sales float,
[services] float,
professional_fees float,
card_payment float,
cash_payment float,
cheque float,
non_vat_expense_count int,
non_vat_expense float,
vat_expense_count int,
vat_expense float
)

insert into @salesandexpenses ([date])
select invoice_date [date] from @sales union select expense_date from @non_vat_expenses union select expense_date from @vat_expenses order by [date]

update slex
set slex.invoice_count = isnull(s.invoice_count, 0), 
slex.dispensed_sales = isnull(s.dispensed_sales, 0), 
slex.adhoc_sales = isnull(s.adhoc_sales, 0), 
slex.services = isnull(s.services, 0), 
slex.professional_fees = isnull(s.professional_fees, 0), 
slex.card_payment = isnull(s.card_payment, 0), 
slex.cash_payment = isnull(s.cash_payment, 0), 
slex.cheque = isnull(s.cheque, 0)
from @salesandexpenses slex
left join @sales s on s.invoice_date = slex.[date]

update slex
set slex.non_vat_expense_count = isnull(ne.expense_count, 0), 
slex.non_vat_expense = isnull(ne.expense, 0)
from @salesandexpenses slex
left join @non_vat_expenses ne on ne.expense_date = slex.[date]

update slex
set slex.vat_expense_count = isnull(ve.expense_count, 0), 
slex.vat_expense = isnull(ve.expense, 0)
from @salesandexpenses slex
left join @vat_expenses ve on ve.expense_date = slex.[date]

select * from @salesandexpenses</CommandText>
