<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="VisionDB">
      <DataSourceReference>/Datasources/VisionDB</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>6e434d62-3d87-4a56-a5b4-6bdb92ba2042</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>VisionDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@practiceId">
            <Value>=Parameters!practiceId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start">
            <Value>=Parameters!start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @PaymentId uniqueidentifier

DECLARE @output TABLE(
Id uniqueidentifier,
patient_name NVARCHAR(200),
patient_number NVARCHAR(20),
payment_date datetime,
invoice_date datetime,
invoice_number NVARCHAR(50),
dispensed_sales float,
adhoc_sales float,
[services] float,
professional_fees float,
previous_balance float,
card_payment float,
cash_payment float,
cheque float,
voucher float,
new_balance float
)

DECLARE @ReportData CURSOR
SET @ReportData = CURSOR FOR
select id.Id from Invoices i
inner join InvoiceDetails id on id.invoice_Id = i.Id
inner join Products p on p.Id = id.product_Id
inner join ProductTypes pt on pt.Id = p.ProductTypeEnum
where i.practice_Id = @practiceId and i.Deleted is null and id.Added &gt; @start and id.Added &lt; @end 
and pt.NegativeValue = 1
OPEN @ReportData
FETCH NEXT
FROM @ReportData INTO @PaymentId
WHILE @@FETCH_STATUS = 0
BEGIN

declare @customerId uniqueidentifier = null
declare @payment_date datetime = null
declare @InvoiceId uniqueidentifier = null;
declare @invoice_date datetime = null
declare @invoice_number nvarchar(50) = null

select @customerId = i.customer_Id, @invoice_date = i.InvoiceDate, @invoice_number = i.InvoiceNumber, @InvoiceId = i.Id from Invoices i 
inner join InvoiceDetails id on id.invoice_Id = i.Id
where id.Id = @PaymentId

declare @patient_name nvarchar(200) = null
declare @patient_number nvarchar(20) = null
select @patient_name = isnull(c.Title, '') + ' ' + c.Firstnames + ' ' + c.Surname, @patient_number = c.Number from Customers c where c.Id = @customerId
IF (@patient_name is null)
BEGIN
	set @patient_name = 'Counter sale'
END

declare @dispensed_sales float = 0
declare @adhoc_sales float = 0
declare @services float = 0
declare @professional_fees float = 0
declare @previous_balance float = 0
declare @card_payment float = 0
declare @cash_payment float = 0
declare @cheque float = 0
declare @voucher float = 0
declare @new_balance float = 0

select @payment_date = max(Added) from InvoiceDetails where invoice_Id = @InvoiceId and Deleted is null and UnitPrice &lt; 0

--frames and lenses are dispensed
select @dispensed_sales = isnull(sum(id.UnitPrice * id.Quantity), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (1, 2)
--contact lenses and tints can be dispensed
select @dispensed_sales = @dispensed_sales + isnull(sum(id.UnitPrice * id.Quantity), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (8, 9) and id.Dispensed = 1

--accessories and sunglasses
select @adhoc_sales = isnull(sum(id.UnitPrice * id.Quantity), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (0, 7)
--contact lenses and tints may not be dispensed in some cases
select @adhoc_sales = @adhoc_sales + isnull(sum(id.UnitPrice * id.Quantity), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (8, 9) and id.Dispensed = 0

--services
select @services = isnull(sum(id.UnitPrice * id.Quantity), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (3)

--professional fees
select @professional_fees = isnull(sum(id.UnitPrice * id.Quantity), 0) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.invoice_Id = @InvoiceId and id.Deleted is null 
and p.ProductTypeEnum in (10)

--card payment
select @card_payment = abs(isnull(sum(id.UnitPrice * id.Quantity), 0)) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.Id = @PaymentId and id.Deleted is null 
and p.ProductTypeEnum in (5) and (p.Name like '%card%' or p.Name like '%pdq%')

--cash payment
select @cash_payment = abs(isnull(sum(id.UnitPrice * id.Quantity), 0)) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.Id = @PaymentId and id.Deleted is null 
and p.ProductTypeEnum in (5) and p.Name like '%cash%'

--cheque payment
select @cheque = abs(isnull(sum(id.UnitPrice * id.Quantity), 0)) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.Id = @PaymentId and id.Deleted is null 
and p.ProductTypeEnum in (5) and (p.Name like '%cheque%' or p.Name like '%chq%')

--voucher
select @voucher = abs(isnull(sum(id.UnitPrice * id.Quantity), 0)) from InvoiceDetails id
inner join Products p on p.Id = id.product_Id 
where id.Id = @PaymentId and id.Deleted is null 
and p.ProductTypeEnum in (6)

set @new_balance = dbo.fn_VDB_InvoiceBalanceIncVAT(@InvoiceId)

set @previous_balance = dbo.fn_VDB_InvoiceTotalIncVAT(@InvoiceId) - @card_payment - @cash_payment - @cheque - @voucher

insert into @output (Id, patient_name, patient_number, payment_date, invoice_date, invoice_number, dispensed_sales, adhoc_sales, [services], professional_fees, previous_balance, card_payment, cash_payment, cheque, voucher, new_balance) 
values (@PaymentId, @patient_name, @patient_number, @payment_date, @invoice_date, @invoice_number, @dispensed_sales, @adhoc_sales, @services, @professional_fees, @previous_balance, @card_payment, @cash_payment, @cheque, @voucher, @new_balance)

FETCH NEXT
FROM @ReportData INTO @PaymentId
END
CLOSE @ReportData
DEALLOCATE @ReportData

select * from @output order by payment_date </CommandText>
