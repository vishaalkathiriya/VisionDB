        </Field>
        <Field Name="voucher">
          <DataField>voucher</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="payment_total">
          <Value>=Fields!card_payment.Value + Fields!cash_payment.Value + Fields!cheque.Value + Fields!voucher.Value</Value>
        </Field>
        <Field Name="new_balance">
          <DataField>new_balance</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
