    <DataSet Name="Data">
      <Query>
        <DataSourceName>VisionDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@practiceId">
            <Value>=Parameters!practiceId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start">
            <Value>=Parameters!start.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end">
            <Value>=Parameters!end.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@includePending">
            <Value>=Parameters!includePending.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@includeReconciled">
            <Value>=Parameters!includeReconciled.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@includeFlagged">
            <Value>=Parameters!includeFlagged.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select id.Added payment_date,
rs.Name reconciliation_status,
i.InvoiceNumber,
p.Name product_name,
c.Title + ' ' + c.Firstnames + ' ' + c.Surname + isnull(' - ' + c.Number, '')  patient_name,
abs(id.UnitPrice) value
from InvoiceDetails id
inner join Invoices i on i.Id = id.invoice_Id inner join Products p on p.Id = id.product_Id inner join ProductTypes pt on pt.Id = p.ProductTypeEnum inner join ReconciliationStatuses rs on rs.Id = id.ReconciliationStatusEnum inner join Customers c on c.Id = i.customer_Id where i.practice_Id = @practiceId and i.Deleted is null and id.Deleted is null and id.Added &gt; @start and id.Added &lt; @end and pt.NegativeValue = 1 and ( (@includePending = 1 and id.ReconciliationStatusEnum = 1) or (@includeReconciled = 1 and id.ReconciliationStatusEnum = 2) or (@includeFlagged = 1 and id.ReconciliationStatusEnum = 3)
)
</CommandText>
      </Query>
